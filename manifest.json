{
  "name": "anno",
  "version": "1.0.0",
  "manifest_version": 3,
  "description": "Flexible annotation for the web. ",
  "action": {},
  "background": {
    "service_worker": "dist/background.js"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+L",
        "mac": "Command+Shift+L"
      }
    }
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["dist/content.js"]
    }
  ],
  "icons": { "16": "icon16.png", "48": "icon48.png", "128": "icon128.png" }
}
